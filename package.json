    "dev": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "checks": "bun run lint && bun run typecheck && bun run test",
    "version": "bun run build",
    "test": "c8 bun bin/test.ts",
    "quick:test": "bun bin/test.ts",
    "vscode:prepublish": "bun run build"
  },
