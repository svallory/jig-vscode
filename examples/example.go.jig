package main

import "fmt"

{{-- :: markdown :: --}}
/*
  # Hello There
*/
{{-- // markdown --}}
func main() {
  // Example for jig-go
  title := "{{ title }}"
  fmt.Printf("Title: %s\n", title)

  @if(debugMode)
    fmt.Println("Debug mode is active")
  @end

  items := []string{
    @each(item in items)
      "{{ item }}",
    @end
  }

  {{-- Jig comment in Go --}}
  fmt.Println(items)
}